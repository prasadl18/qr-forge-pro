{
  "manifest_version": 3,
  "name": "QR Forge Pro - Security & Design Studio",
  "version": "2.0.0",
  "description": "Professional QR generator with enterprise security, password protection, and stunning design",
  "permissions": [
    "activeTab",
    "storage",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}